{
  "name": "Siemor",
  "colors": {
    // Base Colors
    "foreground": "#737c94",
    "errorForeground": "#bf616a",
    "widget.shadow": "#25283310",
    "selection.background": "#b0bcd3",
    "descriptionForeground": "#b0bcd3",

    // Button Control
    "button.background": "#737c94",
    "button.foreground": "#b0bcd3",
    "button.hoverBackground": "#5c6478",

    // Dropdown Control
    "dropdown.background": "#252833",
    "dropdown.border": "#3b404d",
    "dropdown.foreground": "#b0bcd3",

    // Input Control
    "input.background": "#b0bcd305",
    "input.border": "#b0bcd310",
    "input.foreground": "#b0bcd3",
    "input.placeholderForeground": "#737c94",
    "inputOption.activeBorder": "#81a1c1",
    "inputValidation.errorBorder": "#bf616a",
    "inputValidation.infoBorder": "#81a1c1",
    "inputValidation.warningBorder": "#f2db9a",

    // Scroll Bar Control
    "scrollbar.shadow": "#25283300",
    "scrollbarSlider.activeBackground": "#76a3c3",
    "scrollbarSlider.background": "#25283350",
    "scrollbarSlider.hoverBackground": "#25283330",

    // Badge
    "badge.background": "#81a1c1",
    "badge.foreground": "#fff",

    // Progress Bar
    "progressBar.background": "#76a3c3",

    // Lists and Trees
    "list.activeSelectionBackground": "#81a1c130",
    "list.activeSelectionForeground": "#fff",
    "list.focusForeground": "#b0bcd3",
    "list.highlightForeground": "#76a3c380",
    "list.hoverBackground": "#252833",
    "list.hoverForeground": "#b0bcd3",
    "list.inactiveSelectionBackground": "#81a1c130",
    "list.inactiveSelectionForeground": "#b0bcd3",
    "list.inactiveFocusBackground": "#b0bcd3",

    // Status Bar
    "statusBar.background": "#252833",
    "statusBar.border": "#252833",
    "statusBar.debuggingBackground": "#bb6ea7",
    "statusBar.debuggingForeground": "#b0bcd3",
    "statusBar.foreground": "#737c94",
    "statusBar.noFolderBackground": "#252833",
    "statusBarItem.hoverBackground": "#3b404d",

    // Activity Bar
    "activityBar.background": "#252833",
    "activityBar.border": "#252833",
    "activityBar.foreground": "#737c94",
    "activityBarBadge.background": "#81a1c1",
    "activityBarBadge.foreground": "#fff",

    // Sidebar
    "sideBar.background": "#252833",
    "sideBar.border": "#252833",
    "sideBar.foreground": "#737c94",
    "sideBarSectionHeader.background": "#252833",
    "sideBarTitle.foreground": "#b0bcd3",

    // Editor Groups & Tabs
    "editorGroup.border": "#3b404d",
    "editorGroupHeader.tabsBackground": "#252833",
    "tab.activeBorder": "#81a1c160",
    "tab.activeForeground": "#b0bcd3",
    "tab.border": "#252833",
    "tab.hoverBackground": "#81a1c130",
    "tab.inactiveBackground": "#252833",
    "tab.inactiveForeground": "#76a3c390",
    "tab.unfocusedActiveBorder": "#3b404d",
    "tab.unfocusedActiveForeground": "#76a3c330",

    // Editor
    "editor.background": "#252833",
    "editor.foreground": "#b0bcd3",
    "editor.findMatchBackground": "#81a1c160",
    "editor.findMatchHighlightBackground": "#81a1c140",
    "editor.lineHighlightBackground": "#3b404d",
    "editor.rangeHighlightBackground": "#737c94",
    "editor.selectionBackground": "#81a1c160",
    "editor.selectionHighlightBackground": "#81a1c160",
    "editorBracketMatch.background": "#81a1c130",
    "editorBracketMatch.border": "#81a1c130",

    "editorCursor.foreground": "#ffffff",

    "editorIndentGuide.background": "#3b404d",
    "editorLineNumber.foreground": "#3b404d",
    "editorLineNumber.activeForeground": "#3b404d",
    "editorLink.activeForeground": "#b0bcd3",
    "editorMarkerNavigation.background": "#b0bcd305",
    "editorOverviewRuler.border": "#3b404d",
    "editorRuler.foreground": "#b0bcd320",
    "editorWhitespace.foreground": "#3b404d",

    "editorWarning.foreground": "#8dc6bf",
    "editorError.foreground": "#bb6ea7",
    "editorGutter.addedBackground": "#8dc6bf",
    "editorGutter.deletedBackground": "#bb6ea7",
    "editorGutter.modifiedBackground": "#f2db9a",

    "focusBorder": "#b0bcd300",
    "textLink.activeForeground": "#b0bcd3",
    "textLink.foreground": "#76a3c3",

    // Picker
    "pickerGroup.foreground": "#76a3c3",

    // Extensions
    "extensionButton.prominentBackground": "#8dc6bf",
    "extensionButton.prominentHoverBackground": "#8dc6bf",

    // Editor widget
    "editorHoverWidget.background": "#252833",
    "editorHoverWidget.border": "#b0bcd310",
    "editorSuggestWidget.background": "#252833",
    "editorSuggestWidget.border": "#b0bcd310",
    "editorSuggestWidget.foreground": "#b0bcd3",
    "editorSuggestWidget.highlightForeground": "#8fbcbb",
    "editorSuggestWidget.selectedBackground": "#81a1c160",
    "editorWidget.background": "#252833",

    // Peek view colors
    "peekView.border": "#25283330",
    "peekViewEditor.background": "#b0bcd305",
    "peekViewEditor.matchHighlightBackground": "#25283350",
    "peekViewEditorGutter.background": "#b0bcd305",
    "peekViewResult.background": "#b0bcd305",
    "peekViewResult.matchHighlightBackground": "#25283350",
    "peekViewTitle.background": "#b0bcd305",
    "peekViewTitleDescription.foreground": "#b0bcd360",

    // Diff editor
    "diffEditor.insertedTextBackground": "#76a3c380",
    "diffEditor.removedTextBackground": "#bb6ea780",

    // Title bar
    "titleBar.activeBackground": "#252833",
    "titleBar.activeForeground": "#252833",
    "titleBar.border": "#25283360",
    "titleBar.inactiveBackground": "#252833",
    "titleBar.inactiveForeground": "#252833",

    // Terminal
    "terminal.ansiBlack": "#3b4252",
    "terminal.ansiBlue": "#81a1c1",
    "terminal.ansiBrightBlack": "#252833",
    "terminal.ansiBrightBlue": "#81a1c1",
    "terminal.ansiBrightCyan": "#8fbcbb",
    "terminal.ansiBrightGreen": "#8dc6bf",
    "terminal.ansiBrightMagenta": "#bb6ea7",
    "terminal.ansiBrightRed": "#bf616a",
    "terminal.ansiBrightWhite": "#eceff4",
    "terminal.ansiBrightYellow": "#f2db9a",
    "terminal.ansiCyan": "#76a3c3",
    "terminal.ansiGreen": "#8dc6bf",
    "terminal.ansiMagenta": "#bb6ea7",
    "terminal.ansiRed": "#bf616a",
    "terminal.ansiWhite": "#e5e9f0",
    "terminal.ansiYellow": "#f2db9a",
    "terminal.foreground": "#b0bcd3",

    // Panel
    "panel.background": "#252833",
    "panel.border": "#3b404d",
    "panelTitle.activeForeground": "#b0bcd3",
    "panelTitle.inactiveForeground": "#737c94",

    // Git
    "gitDecoration.addedResourceForeground": "#8dc6bf",
    "gitDecoration.conflictingResourceForeground": "#bf616a",
    "gitDecoration.deletedResourceForeground": "#bf616a",
    "gitDecoration.ignoredResourceForeground": "#b0bcd360",
    "gitDecoration.modifiedResourceForeground": "#76a3c3",
    "gitDecoration.untrackedResourceForeground": "#f2db9a",

    // Settings
    "settings.headerForeground": "#76a3c3",
    "settings.modifiedItemIndicator": "#f2db9a",
    "settings.checkboxBackground": "#76a3c3",
    "settings.checkboxForeground": "#76a3c3",
    "settings.checkboxBorder": "#76a3c3",

    // Other
    "debugToolBar.background": "#252833",

  },
  "tokenColors": [
    {
      "scope": "string",
      "settings": {
        "foreground": "#8dc6bf"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#f2db9a"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#76a3c3"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "constant.language.json, constant.language.import-export-all",
      "settings": {
        "foreground": "#8dc6bf"
      }
    },
    {
      "scope": "support.class",
      "settings": {
        "foreground": "#76a3c3"
      }
    },
    {
      "scope": "support.variable, support.constant",
      "settings": {
        "foreground": "#b0bcd3"
      }
    },
    {
      "scope": "variable, meta.definition.variable entity.name.function",
      "settings": {
        "foreground": "#b0bcd3"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "variable.object.property, variable.other.object.property",
      "settings": {
        "foreground": "#8dc6bf"
      }
    },
    {
      "scope": "support.variable.property, variable.other.property",
      "settings": {
        "foreground": "#bb6ea7"
      }
    },
    {
      "scope": "variable.language.this",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "modifier, support.type.object",
      "settings": {
        "foreground": "#8dc6bf"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#f2db9a"
      }
    },
    {
      "scope": "keyword.control.loop, keyword.control.conditional, keyword.control.trycatch",
      "settings": {
        "foreground": "#f2db9a"
      }
    },
    {
      "scope": "keyword.control.import, keyword.control.from, keyword.control.export, keyword.control.default",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword.control.flow",
      "settings": {
        "foreground": "#bb6ea7",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "entity.name.function, support.function",
      "settings": {
        "foreground": "#76a3c3"
      }
    },
    {
      "scope": "storage.type, storage.modifier",
      "settings": {
        "foreground": "#8dc6bf",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "support.module, support.node",
      "settings": {
        "foreground": "#b0bcd3",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#bb6ea7"
      }
    },
    {
      "scope": "entity.name.type, entity.other.inherited-class",
      "settings": {
        "foreground": "#bb6ea7"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#76a3c370",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "entity.name.type.class",
      "settings": {
        "foreground": "#bb6ea7"
      }
    },
    {
      "scope": "meta.definition.method entity.name.function",
      "settings": {
        "foreground": "#76a3c3"
      }
    },
    {
      "scope": "meta.function entity.name.function",
      "settings": {
        "foreground": "#76a3c3"
      }
    },
    {
      "scope": "template.expression.begin, template.expression.end",
      "settings": {
        "foreground": "#8dc6bf"
      }
    },
    {
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#bb6ea7"
      }
    },
    {
      "scope": "meta.object-literal.key, meta.object-literal.key string, support.type.property-name.json",
      "settings": {
        "foreground": "#76a3c3",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "entity.other.attribute-name.class",
      "settings": {
        "foreground": "#8dc6bf"
      }
    },
    {
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#76a3c3"
      }
    },
    {
      "scope": "source.css",
      "settings": {
        "foreground": "#f2db9a"
      }
    },
    {
      "scope": "meta.tag, punctuation.definition.tag",
      "settings": {
        "foreground": "#8dc6bf"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#76a3c3"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#f2db9a"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#8dc6bf"
      }
    },
    {
      "scope": "text.html.markdown meta.link.inline, meta.link.reference",
      "settings": {
        "foreground": "#b0bcd3"
      }
    },
    {
      "scope": "text.html.markdown beginning.punctuation.definition.list",
      "settings": {
        "foreground": "#8dc6bf"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#b0bcd3"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "foreground": "#b0bcd3",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.bold markup.italic, markup.italic markup.bold",
      "settings": {
        "foreground": "#b0bcd3"
      }
    },
    {
      "scope": "keyword.other.definition.ini",
      "settings": {
        "foreground": "#b0bcd3"
      }
    },
    {
      "scope": "entity.name.section.group-title.ini",
      "settings": {
        "foreground": "#8dc6bf"
      }
    },
    {
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#76a3c3"
      }
    },
    {
      "scope": "storage.modifier.ts, entity.name.type.module.ts, support.type.primitive.ts",
      "settings": {
        "foreground": "#76a3c3"
      }
    },
    {
      "scope": "entity.name.type.instance.jsdoc",
      "settings": {
        "foreground": "#76a3c3"
      }
    },
    {
      "scope": "punctuation.separator.parameter",
      "settings": {
        "foreground": "#76a3c370"
      }
    },
    {
      "scope": "meta.brace, punctuation.definition.parameters, punctuation.definition.block, punctuation.terminator.statement, punctuation.separator.comma, punctuation.section.embedded",
      "settings": {
        "foreground": "#76a3c370"
      }
    }
  ]
}
